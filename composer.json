{
    "name": "emnabs/yii2-jcrop",
    "description": " yii2 image cropping extension ",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension", "jcrop", "crop image"],
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Ehsan Rezaei",
            "email": "mr.rezaee121@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-imagine": "*",
        "bower-asset/Jcrop": "=0.9.12",
        "bower-asset/simple-ajax-uploader": "@dev"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "emhome\\jcrop\\": "src"
        }
    }
}
